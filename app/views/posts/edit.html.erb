<%= title edit(:post) %>

<%= render 'form', post:@post, date:@date, month:@month %>

<%= form_tag new_post_path, :method => :get do %>
  <%= hidden_field_tag :date, @date %>
  <%= hidden_field_tag :month, @month %>
  <%= submit_tag t('helpers.submit.cancel'), :class => :cancel %>
<% end %>

<div class="clear"></div>
