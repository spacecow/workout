<script type="text/javascript" charset="utf-8">
  $(function(){
    new Highcharts.Chart({
      chart: {
        renderTo: "chart"
      },
      title: {
        text: "Top Score"
      },
      xAxis: {
        type: "datetime"
      },
      yAxis: {
        title: {
          text: "min"
        }
      },
      series: [{
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= @topentries.first_date %>,
        data: <%= @topentries.map(&:score) %>
      }]
    });
  });
</script>

<div id="chart" style="width:560px; height:300px;"></div>
